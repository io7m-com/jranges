        <c:change compatible="false" date="2022-04-09T00:00:00+00:00" summary="Require JDK 17"/>
      </c:changes>
    </c:release>
    <c:release date="2024-05-11T14:35:47+00:00" is-open="true" ticket-system="com.github.io7m.jranges" version="6.0.0">
      <c:changes>
        <c:change date="2024-05-11T00:00:00+00:00" summary="Move to new organization."/>
        <c:change compatible="false" date="2024-05-11T14:35:47+00:00" summary="Require JDK 21."/>
      </c:changes>
    </c:release>
  </c:releases>
