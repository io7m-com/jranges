    <c:release date="2014-06-22T00:00:00+00:00" is-open="false" ticket-system="com.io7m.fossil.jranges" version="1.0.0">
      <c:changes>
        <c:change date="2014-06-22T00:00:00+00:00" summary="Initial release."/>
      </c:changes>
    </c:release>
    <c:release date="2015-07-19T00:00:00+00:00" is-open="false" ticket-system="com.io7m.fossil.jranges" version="2.0.0">
      <c:changes>
        <c:change compatible="false" date="2015-07-19T00:00:00+00:00" summary="Correct parameter type of RangeInclusiveD.includesValue()."/>
        <c:change compatible="false" date="2015-07-19T00:00:00+00:00" summary="Move all predefined ranges to Ranges."/>
        <c:change compatible="false" date="2015-07-19T00:00:00+00:00" summary="Suffix all previously unsuffixed method names with Long for uniformity."/>
        <c:change date="2015-07-19T00:00:00+00:00" summary="Add JCIP annotations."/>
        <c:change date="2014-12-03T00:00:00+00:00" summary="Fix javadoc comments for Java 8's doclint."/>
        <c:change date="2014-12-03T00:00:00+00:00" summary="Ensure source jars for unit tests are created."/>
      </c:changes>
    </c:release>
    <c:release date="2015-08-09T00:00:00+00:00" is-open="false" ticket-system="com.github.io7m.jranges" version="2.0.1">
      <c:changes>
        <c:change date="2015-08-09T00:00:00+00:00" summary="Migrate to GitHub."/>
      </c:changes>
    </c:release>
    <c:release date="2016-08-03T00:00:00+00:00" is-open="false" ticket-system="com.github.io7m.jranges" version="2.0.2">
      <c:changes>
        <c:change date="2016-08-03T00:00:00+00:00" summary="Update POM files to 2016 standards. Update plugins."/>
        <c:change date="2016-08-03T00:00:00+00:00" summary="The packaged jar is now an OSGi bundle."/>
        <c:change compatible="false" date="2016-08-03T00:00:00+00:00" summary="The package will no longer compile on JDK 6 due to Checkstyle, but the compiled code is still JDK 6 compatible."/>
      </c:changes>
    </c:release>
    <c:release date="2016-08-04T00:00:00+00:00" is-open="false" ticket-system="com.github.io7m.jranges" version="2.0.3">
      <c:changes>
        <c:change date="2016-08-04T00:00:00+00:00" summary="Switch to using com.io7m.jcip bundle to fix the produced OSGi bundle.">
          <c:tickets>
            <c:ticket id="1"/>
          </c:tickets>
        </c:change>
      </c:changes>
    </c:release>
    <c:release date="2017-04-06T00:00:00+00:00" is-open="false" ticket-system="com.github.io7m.jranges" version="2.0.4">
      <c:changes>
        <c:change date="2017-04-06T00:00:00+00:00" summary="Use primogenitor, renaming all modules to the new OSGi conventions."/>
      </c:changes>
    </c:release>
    <c:release date="2017-11-15T00:00:00+00:00" is-open="false" ticket-system="com.github.io7m.jranges" version="3.0.0">
      <c:changes>
        <c:change compatible="false" date="2017-11-15T00:00:00+00:00" summary="Replace implementation with Immutables.org generated types"/>
        <c:change compatible="false" date="2017-11-15T00:00:00+00:00" summary="All modules are now Java 9 modules. JDK 9 is now required."/>
        <c:change compatible="false" date="2017-11-15T00:00:00+00:00" summary="Removed dependency on com.io7m.jnull."/>
        <c:change compatible="false" date="2017-11-15T00:00:00+00:00" summary="Removed dependency on com.io7m.jequality."/>
      </c:changes>
    </c:release>
    <c:release date="2019-03-01T00:00:00+00:00" is-open="false" ticket-system="com.github.io7m.jranges" version="4.0.0">
      <c:changes>
        <c:change date="2019-03-01T00:00:00+00:00" summary="Add half-open ranges">
          <c:tickets>
            <c:ticket id="3"/>
          </c:tickets>
        </c:change>
        <c:change compatible="false" date="2019-03-01T00:00:00+00:00" summary="Require JDK 11"/>
      </c:changes>
    </c:release>
    <c:release date="2022-04-09T00:00:00+00:00" is-open="false" ticket-system="com.github.io7m.jranges" version="5.0.0">
